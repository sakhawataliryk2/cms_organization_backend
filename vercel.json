{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/(.*)", "dest": "/index.js" },
    { "src": "/test-db", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "crons": [
    { "path": "/api/cron/archive-cleanup", "schedule": "40 17 * * *" }
  ],
  "env": { "NODE_ENV": "production" }
}
